{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"globals": {
		"document": true,
		"window": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"react/prop-types": 1,
		"react/react-in-jsx-scope": 0,
		"react/jsx-uses-react": 1,
		"react/jsx-uses-vars": 1
	}
}
